//描述：暴力枚举
int* twoSum(int* nums, int numsSize, int target, int* returnSize)
{

    for(int m=0; m<numsSize; m++)
        {
            for(int n=m+1; n<numsSize; n++)
            {
                if(nums[m]+nums[n]==target)
                {
                    int* size=malloc(sizeof(int) * 2);//申请8个字节的空间
                    size[0]=m;size[1]=n;
                    *returnSize = 2;
                    return size;
                }
        }
    }

    return 0;
}
